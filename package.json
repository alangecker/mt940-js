{
  "name": "mt940-js",
  "version": "0.1.0",
  "description": "An isomorphic Javascript library for working with MT940 format",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-jest": "^14.1.0",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^3.5.0",
    "jest-cli": "^15.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^1.5.0",
    "typescript": "^2.0.7",
    "typings": "^1.5.0",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest --no-cache",
    "typings": "./node_modules/.bin/typings install",
    "lint_src": "./node_modules/.bin/tslint \"src/**/*.*\"",
    "lint_test": "./node_modules/.bin/eslint \"test/unit/**/*.*\"",
    "build": "npm run lint && rm -rf lib && ./node_modules/.bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webschik/mt940-js.git"
  },
  "keywords": [
    "JS",
    "isomorphic",
    "mt940"
  ],
  "author": "",
  "license": "MIT",
  "jest": {
    "automock": false,
    "collectCoverage": true,
    "coverageDirectory": "test/coverage",
    "coverageReporters": [
      "cobertura"
    ],
    "testDirectoryName": "test/unit",
    "scriptPreprocessor": "<rootDir>/jest/preprocessor.js",
    "setupTestFrameworkScriptFile": "<rootDir>/jest/config.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/e2e/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules"
    ],
    "testFileExtensions": [
      "ts",
      "tsx"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}